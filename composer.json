{
    "name":"christianbarkowsky/contao-tiny-compress-images",
    "description":"This plugin automatically optimizes your images by integrating with the popular image compression services TinyJPG and TinyPNG.",
    "keywords":["contao", "images", "compress", "upload", "tiny"],
    "type":"contao-module",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"Christian Barkowsky",
            "email":"hallo@christianbarkowsky.de",
            "homepage":"http://christianbarkowsky.de",
            "role":"Developer"
        }
    ],
    "support":{
            "email":"hallo@christianbarkowsky.de",
            "issues":"https://github.com/christianbarkowsky/contao-tiny-compress-images/issues",
            "source":"https://github.com/christianbarkowsky/contao-tiny-compress-images"
    },
    "require":{
            "php":">=5.3",
            "contao/core-bundle": "~3.2 || ~4.0",
            "contao-community-alliance/composer-plugin": "~2.4 || ~3.0"
    },
    "extra": {
        "contao": {
            "sources": {
                "contao": "system/modules/tiny-compress-images"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Barkowsky\\": "src/"
        }
    }
}